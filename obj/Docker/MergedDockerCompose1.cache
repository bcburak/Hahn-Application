services:
  hahn.applicatonprocess.web:
    build:
      context: C:\Users\Burak.Celik\source\Hahn.ApplicatonProcess.Application
      dockerfile: Hahn.ApplicatonProcess.Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Hahn.ApplicatonProcess.Web
      target: base
    container_name: Hahn.ApplicatonProcess.Web
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: hahnapplicatonprocessweb:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Hahn.ApplicatonProcess.Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\Burak.Celik\source\Hahn.ApplicatonProcess.Application\Hahn.ApplicatonProcess.Web:/app:rw
    - C:\Users\Burak.Celik\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Burak.Celik\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Burak.Celik\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Burak.Celik\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Burak.Celik\source\Hahn.ApplicatonProcess.Application:/src:rw
